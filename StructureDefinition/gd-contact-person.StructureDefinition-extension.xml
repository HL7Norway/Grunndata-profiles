<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="gd-contact-person" />
  <url value="http://ehelse.no/fhir/StructureDefinition/gd-contact-person" />
  <version value="3.0.0" />
  <name value="GdContactPerson" />
  <status value="draft" />
  <date value="2021-06-17" />
  <description value="An identified individual is the contact, the resource contains identifying information regarding the person&#xD;&#xA;&#xD;&#xA;Det er en privatperson som er kontaktperson og entiteten inneholder enten en folkeregisteridentifikator eller identifiserende informasjon som fødselsdato og navn" />
  <fhirVersion value="4.0.1" />
  <mapping>
    <identity value="rim" />
    <uri value="http://hl7.org/v3" />
    <name value="RIM Mapping" />
  </mapping>
  <kind value="complex-type" />
  <abstract value="false" />
  <context>
    <type value="element" />
    <expression value="Person" />
  </context>
  <type value="Extension" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Extension" />
  <derivation value="constraint" />
  <differential>
    <element id="Extension">
      <path value="Extension" />
      <short value="An identified individual handles the residure" />
      <definition value="An identified individual handles the residure&#xD;&#xA;&#xD;&#xA;Det er en norsk eller utenlandsk privatperson som tar hånd om boet.&#xD;&#xA;individ, enkeltmenneske&#xD;&#xA;En person kan enten identifiseres med sikkerhet ved fødsels- eller d-nummer, eller med mindre sikkerhet ved navn og/eller fødselsdato." />
      <alias value="PersonSomKontakt" />
      <mapping>
        <identity value="freg" />
        <map value="PersonSomKontakt" />
      </mapping>
    </element>
    <element id="Extension.extension">
      <path value="Extension.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Extension.extension:personIdentifier">
      <path value="Extension.extension" />
      <sliceName value="personIdentifier" />
      <short value="Identifier for the person (fødselsnummer eller d-nummer)" />
      <definition value="Identifier for the person (fødselsnummer eller d-nummer)&#xD;&#xA;&#xD;&#xA;Fødselsnummer: personidentifikasjonsnummer for de som er, eller har vært, bosatt i Norge, samt personer som er &quot;ikke bosatt&quot;&#xD;&#xA;d-nummer: identifikasjonsnummer som tildeles personer med midlertidig tilknytning til Norge, det vil si som ikke er bosatt i Norge&#xD;&#xA;Folkeregisteridentifikatoren til kontaktpersonen hvis denne finnes i Folkeregisteret." />
      <alias value="foedselsEllerDNummer" />
      <max value="1" />
      <mustSupport value="true" />
      <mapping>
        <identity value="freg" />
        <map value="foedselsEllerDNummer" />
      </mapping>
    </element>
    <element id="Extension.extension:personIdentifier.url">
      <path value="Extension.extension.url" />
      <fixedUri value="personIdentifier" />
    </element>
    <element id="Extension.extension:personIdentifier.value[x]">
      <path value="Extension.extension.value[x]" />
      <type>
        <code value="string" />
      </type>
    </element>
    <element id="Extension.extension:birthDate">
      <path value="Extension.extension" />
      <sliceName value="birthDate" />
      <short value="birth date of the person" />
      <definition value="birth date of the person" />
      <alias value="foedselsdato" />
      <max value="1" />
      <mustSupport value="true" />
      <mapping>
        <identity value="freg" />
        <map value="foedselsdato" />
      </mapping>
    </element>
    <element id="Extension.extension:birthDate.url">
      <path value="Extension.extension.url" />
      <fixedUri value="birthDate" />
    </element>
    <element id="Extension.extension:birthDate.value[x]">
      <path value="Extension.extension.value[x]" />
      <type>
        <code value="date" />
      </type>
    </element>
    <element id="Extension.extension:personName">
      <path value="Extension.extension" />
      <sliceName value="personName" />
      <short value="Name of the person" />
      <definition value="Name of the person&#xD;&#xA;&#xD;&#xA;Kontaktpersonenes navn. Se 5.26.2&#xD;&#xA;navn på person som består av fornavn og ett enkelt eller dobbelt etternavn og kan i tillegg bestå av mellomnavn" />
      <alias value="personnavn" />
      <max value="1" />
      <mustSupport value="true" />
      <mapping>
        <identity value="freg" />
        <map value="personnavn" />
      </mapping>
    </element>
    <element id="Extension.extension:personName.url">
      <path value="Extension.extension.url" />
      <fixedUri value="personName" />
    </element>
    <element id="Extension.extension:personName.value[x]">
      <path value="Extension.extension.value[x]" />
      <type>
        <code value="HumanName" />
        <profile value="http://hl7.no/fhir/StructureDefinition/no-basis-HumanName" />
      </type>
    </element>
    <element id="Extension.extension:personName.value[x].extension">
      <path value="Extension.extension.value[x].extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Extension.extension:personName.value[x].extension:middlename">
      <path value="Extension.extension.value[x].extension" />
      <sliceName value="middlename" />
      <mustSupport value="true" />
    </element>
    <element id="Extension.extension:personName.value[x].use">
      <path value="Extension.extension.value[x].use" />
      <max value="0" />
    </element>
    <element id="Extension.extension:personName.value[x].text">
      <path value="Extension.extension.value[x].text" />
      <max value="0" />
    </element>
    <element id="Extension.extension:personName.value[x].family">
      <path value="Extension.extension.value[x].family" />
      <min value="1" />
      <mustSupport value="true" />
    </element>
    <element id="Extension.extension:personName.value[x].given">
      <path value="Extension.extension.value[x].given" />
      <min value="1" />
      <max value="1" />
      <mustSupport value="true" />
    </element>
    <element id="Extension.extension:personName.value[x].prefix">
      <path value="Extension.extension.value[x].prefix" />
      <max value="0" />
    </element>
    <element id="Extension.extension:personName.value[x].suffix">
      <path value="Extension.extension.value[x].suffix" />
      <max value="0" />
    </element>
    <element id="Extension.extension:personName.value[x].period">
      <path value="Extension.extension.value[x].period" />
      <max value="0" />
    </element>
    <element id="Extension.url">
      <path value="Extension.url" />
      <fixedUri value="http://ehelse.no/fhir/StructureDefinition/gd-contact-person" />
    </element>
    <element id="Extension.value[x]">
      <path value="Extension.value[x]" />
      <max value="0" />
    </element>
  </differential>
</StructureDefinition>